{
  "plugins": ["@oursky"],
  "parserOptions": {
    "sourceType": "module",
    "project": ["tsconfig.json", "packages/*/tsconfig.json"]
  },
  "env": {
    "node": true,
    "es6": true,
    "jest": true
  },
  "extends": [
    "plugin:@oursky/eslint",
    "plugin:@oursky/oursky",
    "plugin:@oursky/typescript",
    "prettier/@typescript-eslint",
    "plugin:prettier/recommended"
  ]
}
